	keywords = {/unread, Ceramics, Applied physics},
	pages = {1--10},
	file = {Xiao et al_2024_Distinct amorphization resistance in high-entropy MAX-phases (Ti, M)2AlC (M=Nb,.pdf:D\:\\OneDrive\\ZoteroBase\\Xiao et al_2024_Distinct amorphization resistance in high-entropy MAX-phases (Ti, M)2AlC (M=Nb,.pdf:application/pdf},
