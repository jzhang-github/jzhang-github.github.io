	journal = {npj Computational Materials},
	author = {Xiao, Hao and Zhao, Shuang and Zhang, Jun and Zhao, Shijun and Li, Youbing and Chen, Ke and Cao, Liuxuan and Wang, Yugang and Huang, Qing and Wang, Chenxu},
